{
    "name": "attendly",
    "version": "1.0.0",
    "private": true,
    "description": "Backend API with Prisma ORM",
    "main": "index.js",
    "scripts": {
        "start": "npx prisma generate --no-engine && node src/index.js",
        "dev": "nodemon index.js",
        "db:migrate": "npx prisma migrate dev",
        "db:generate": "npx prisma generate",
        "db:studio": "npx prisma studio",
        "db:seed": "node prisma/seed.js",
        "db:push": "npx prisma db push",
        "db:reset": "npx prisma migrate reset",
        "db:deploy": "npx prisma migrate deploy",
        "postinstall": "npx prisma generate"
    },
    "dependencies": {
        "@prisma/client": "^6.12.0",
        "axios": "^1.11.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.21.1",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.16.3"
    },
    "devDependencies": {
        "eslint": "^8.51.0",
        "nodemon": "^3.1.3",
        "prisma": "^6.12.0"
    },
    "keywords": [
        "nodejs",
        "express",
        "prisma",
        "postgresql",
        "api",
        "attendance"
    ]
}
